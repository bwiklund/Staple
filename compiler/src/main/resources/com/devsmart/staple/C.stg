
classTypeDecl(name, parent, functions) ::= <<
struct <name>Class {
  <parent>
  <functions; separator=";\n">
};


>>

classObjDecl(name, parent, fields) ::= <<
struct <name> {
  <parent>
  <fields; separator="\n">
};


>>

functionType(return, name, args) ::= <<
<return> (*<name>)(<args; separator=", ">)
>>