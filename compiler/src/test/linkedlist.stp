
namespace org.staple

class LinkedList {

  class Node {
    Node* next;
    Node* previous;
    Object* data;
  }

  uint size;
  Node* head;
  Node* tail;

  void add(Object* obj) {
    Node* newNode;
    newNode = new Node();
    newNode.data = obj;
    newNode.previous = this.head;

    this.head.next = newNode;
    this.head = newNode;
    this.size++;
  }

  void deleteFirst(){
    if(this.head != null) {
      this.head.previous = null;
      this.head = null;
      this.size--;
    }
  }
}

