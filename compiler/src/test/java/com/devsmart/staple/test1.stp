
namespace com.devsmart.staple.test;

extern int printf(byte* fmt, ... );

int main(int x, int y) {
  y = x + y;
  int r = fib(y);
  printf("fib 8: %d\n", r);
  return 0;
}

int fib(int x) {
	if(x == 0) {
	  return 0;
	} else if(x == 1) {
	  return 1;
	} else {
	  return fib(x-1) + fib(x-2);
	}
}


